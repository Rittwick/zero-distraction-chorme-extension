{
    "manifest_version": 2,
    "name":"Zero Distraction",
    "version": "1.0",
    "description": "Open current tab in a seperate window",
    "icons":{
        "128": "icon128.png",
        "48": "icon128.png",
        "16": "icon128.png"
    },
    "browser_action":{
        "default_icon":"icon128.png",
        "default_popup": "popup.html",
        "default_title": "Zero Distraction"
    },
    "permissions": [
        "activeTab",
        "tabs"
    ]
}